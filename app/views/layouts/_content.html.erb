                <div class="form-item">
                  <p class="box-search__datepicker"><span><%= t "content.when" %></span>
                    <input type="text" id="datepicker" placeholder="When would you like to go ?"/>
                    <input type="hidden" id="test"/>
                  </p>
                </div><!-- End / form-item -->
                <a class="md-btn md-btn--primary md-btn--pill " href="#"><%= t "content.search" %>
                </a>
              </div><!-- End / box-search -->

            </div>
          </div>
        </section>
        <!-- End / Section -->
