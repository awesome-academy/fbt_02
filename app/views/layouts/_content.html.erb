                <div class="form-item">
                  <label class="form__label"><%= t "content.what" %></label>
                  <select class="select__module" placeholder="select">)
                    <option value="0"><%= t "content.answer_what" %></option>
                    <option value="1">data #1</option>
                    <option value="2">data #2</option>
                    <option value="3">data #3</option>
                    <option value="4">data #4</option>
                    <option value="5">data #5</option>
                  </select>
                </div>
                <!-- End / form-item -->
                <!-- form-item -->
                <div class="form-item">
                  <label class="form__label"><%= t "content.where" %></label>
                  <select class="select__module" placeholder="select">)
                    <option value="0"><%= t "content.answer_where" %></option>
                    <option value="1">data #1</option>
                    <option value="2">data #2</option>
                    <option value="3">data #3</option>
                    <option value="4">data #4</option>
                    <option value="5">data #5</option>
                  </select>
                </div>
                <!-- End / form-item -->
