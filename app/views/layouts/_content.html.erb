      <div class="swiper-thumbnails__module swiper-container awe-skin-dark hero__thumbails" data-options='{"spaceBetween":0}'>
        <div class="swiper-wrapper">
               <!--  <div class="hero__item" style="background-image: url('/images/hero/1.jpg');"><span>Bhutan</span></div>
                <div class="hero__item" style="background-image: url('/images/hero/2.jpg');"><span>Panama</span></div>
                <div class="hero__item" style="background-image: url('/images/hero/3.jpg');"><span>Cameroon</span></div> -->
