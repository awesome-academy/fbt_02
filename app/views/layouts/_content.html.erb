                          <div class="box-image" href="/images/image_box_1/1.jpg">
                            <div>
                              <%= link_to "/images/image_box_1/1.jpg", class: "box-image__bg", style: "background-image: url('/images/image_box_1/1.jpg');", "data-effect": "mfp-zoom-in" do %>
